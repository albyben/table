{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "kd-ui": [
        "components/index.tsx"
      ],
      "kd-ui/es/*": [
        "components/*"
      ]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "es6",
    "lib": [
      "dom",
      "es2017",
      "dom.Iterable"
    ],
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "docs"
  ]
}
